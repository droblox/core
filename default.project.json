{
  "name": "Droblox Core",
  "globIgnorePaths": ["**/package.json", "**/tsconfig.json"],
  "tree": {
    "$className": "DataModel",
    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "TypeScript": {
        "$path": "include",
        "node_modules": {
          "$path": "../../node_modules/@rbxts"
        }
      },
      "DrobloxCore": {
        "$path": "dist"
      }
    }
  }
}
